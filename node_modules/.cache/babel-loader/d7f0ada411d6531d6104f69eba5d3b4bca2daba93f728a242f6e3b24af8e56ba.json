{"ast":null,"code":"export default {\n  name: 'HomePage',\n  data() {\n    return {\n      activeIndex: '1',\n      hotJobs: [{\n        id: 1,\n        title: '前端开发工程师',\n        company: '科技有限公司',\n        salary: '15k-25k',\n        location: '北京'\n      }, {\n        id: 2,\n        title: '后端开发工程师',\n        company: '互联网有限公司',\n        salary: '18k-30k',\n        location: '上海'\n      }, {\n        id: 3,\n        title: '产品经理',\n        company: '软件有限公司',\n        salary: '20k-35k',\n        location: '广州'\n      }, {\n        id: 4,\n        title: 'UI设计师',\n        company: '设计有限公司',\n        salary: '12k-22k',\n        location: '深圳'\n      }],\n      hotCompanies: [{\n        id: 1,\n        name: '科技有限公司',\n        industry: '互联网',\n        size: '500-1000人'\n      }, {\n        id: 2,\n        name: '互联网有限公司',\n        industry: '软件',\n        size: '1000-2000人'\n      }, {\n        id: 3,\n        name: '软件有限公司',\n        industry: '科技',\n        size: '2000-5000人'\n      }, {\n        id: 4,\n        name: '设计有限公司',\n        industry: '设计',\n        size: '100-500人'\n      }]\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return this.$store.getters.isLoggedIn;\n    },\n    currentUser() {\n      return this.$store.getters.currentUser;\n    }\n  },\n  methods: {\n    handleSelect(key) {\n      switch (key) {\n        case '1':\n          this.$router.push('/');\n          break;\n        case '2':\n          this.$router.push('/jobs');\n          break;\n        case '3':\n          this.$router.push('/companies');\n          break;\n        case '4':\n          this.$router.push('/login');\n          break;\n        case '5':\n          this.$router.push('/register');\n          break;\n        case '6-1':\n          this.$router.push('/user');\n          break;\n        case '6-2':\n          this.$router.push('/admin');\n          break;\n      }\n    },\n    logout() {\n      this.$store.dispatch('logout');\n      this.$message.success('退出登录成功');\n      this.$router.push('/');\n    },\n    goToJobDetail(id) {\n      this.$router.push(`/jobs/${id}`);\n    },\n    goToCompanyDetail(id) {\n      this.$router.push(`/companies/${id}`);\n    },\n    getHotJobs() {\n      this.$axios.get('/jobs').then(response => {\n        // 模拟企业名称，实际项目中需要关联企业表\n        this.hotJobs = response.data.map(job => ({\n          ...job,\n          company: '企业名称 ' + Math.floor(Math.random() * 100)\n        }));\n      }).catch(error => {\n        console.error('获取岗位失败:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeIndex","hotJobs","id","title","company","salary","location","hotCompanies","industry","size","computed","isLoggedIn","$store","getters","currentUser","methods","handleSelect","key","$router","push","logout","dispatch","$message","success","goToJobDetail","goToCompanyDetail","getHotJobs","$axios","get","then","response","map","job","Math","floor","random","catch","error","console"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <!-- 导航栏 -->\n    <el-menu :default-active=\"activeIndex\" class=\"el-menu-demo\" mode=\"horizontal\" @select=\"handleSelect\">\n      <el-menu-item index=\"1\">首页</el-menu-item>\n      <el-menu-item index=\"2\">职位</el-menu-item>\n      <el-menu-item index=\"3\">企业</el-menu-item>\n      <el-menu-item index=\"4\" v-if=\"!isLoggedIn\">登录</el-menu-item>\n      <el-menu-item index=\"5\" v-if=\"!isLoggedIn\">注册</el-menu-item>\n      <el-submenu index=\"6\" v-if=\"isLoggedIn\">\n        <template slot=\"title\">{{ currentUser.username }}</template>\n        <el-menu-item index=\"6-1\">个人中心</el-menu-item>\n        <el-menu-item index=\"6-2\" v-if=\"currentUser.role === 2\">管理员面板</el-menu-item>\n        <el-menu-item index=\"6-3\" @click=\"logout\">退出登录</el-menu-item>\n      </el-submenu>\n    </el-menu>\n\n    <!-- 轮播图 -->\n    <el-carousel height=\"400px\">\n      <el-carousel-item v-for=\"item in 3\" :key=\"item\">\n        <img :src=\"`https://picsum.photos/1200/400?random=${item}`\" alt=\"轮播图\" style=\"width: 100%; height: 100%; object-fit: cover;\">\n      </el-carousel-item>\n    </el-carousel>\n\n    <!-- 热门职位 -->\n    <div class=\"hot-jobs\">\n      <h2>热门职位</h2>\n      <el-row :gutter=\"20\">\n        <el-col :span=\"6\" v-for=\"job in hotJobs\" :key=\"job.id\">\n          <el-card class=\"job-card\">\n            <div slot=\"header\" class=\"clearfix\">\n              <span>{{ job.title }}</span>\n            </div>\n            <div class=\"job-info\">\n              <p>{{ job.company }}</p>\n              <p>{{ job.salary }}</p>\n              <p>{{ job.location }}</p>\n            </div>\n            <el-button type=\"primary\" size=\"small\" @click=\"goToJobDetail(job.id)\">查看详情</el-button>\n          </el-card>\n        </el-col>\n      </el-row>\n    </div>\n\n    <!-- 热门岗位 -->\n    <div class=\"hot-jobs\">\n      <h3>热门岗位</h3>\n      <div class=\"job-list\">\n        <el-card class=\"job-card\" v-for=\"job in hotJobs\" :key=\"job.id\" @click=\"goToJobDetail(job.id)\">\n          <div class=\"job-info\">\n            <h4>{{ job.title }}</h4>\n            <div class=\"job-meta\">\n              <span class=\"salary\">{{ job.salary }}</span>\n              <span>{{ job.location }}</span>\n            </div>\n            <div class=\"company-name\">{{ job.company }}</div>\n          </div>\n        </el-card>\n      </div>\n    </div>\n\n    <!-- 热门企业 -->\n    <div class=\"hot-companies\">\n      <h2>热门企业</h2>\n      <el-row :gutter=\"20\">\n        <el-col :span=\"6\" v-for=\"company in hotCompanies\" :key=\"company.id\">\n          <el-card class=\"company-card\">\n            <div class=\"company-info\">\n              <img :src=\"`https://picsum.photos/200/200?random=${company.id}`\" alt=\"企业logo\" class=\"company-logo\">\n              <h3>{{ company.name }}</h3>\n              <p>{{ company.industry }}</p>\n              <p>{{ company.size }}</p>\n            </div>\n            <el-button type=\"primary\" size=\"small\" @click=\"goToCompanyDetail(company.id)\">查看详情</el-button>\n          </el-card>\n        </el-col>\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      activeIndex: '1',\n      hotJobs: [\n        { id: 1, title: '前端开发工程师', company: '科技有限公司', salary: '15k-25k', location: '北京' },\n        { id: 2, title: '后端开发工程师', company: '互联网有限公司', salary: '18k-30k', location: '上海' },\n        { id: 3, title: '产品经理', company: '软件有限公司', salary: '20k-35k', location: '广州' },\n        { id: 4, title: 'UI设计师', company: '设计有限公司', salary: '12k-22k', location: '深圳' }\n      ],\n      hotCompanies: [\n        { id: 1, name: '科技有限公司', industry: '互联网', size: '500-1000人' },\n        { id: 2, name: '互联网有限公司', industry: '软件', size: '1000-2000人' },\n        { id: 3, name: '软件有限公司', industry: '科技', size: '2000-5000人' },\n        { id: 4, name: '设计有限公司', industry: '设计', size: '100-500人' }\n      ]\n    }\n  },\n  computed: {\n    isLoggedIn() {\n      return this.$store.getters.isLoggedIn;\n    },\n    currentUser() {\n      return this.$store.getters.currentUser;\n    }\n  },\n  methods: {\n    handleSelect(key) {\n      switch(key) {\n        case '1':\n          this.$router.push('/');\n          break;\n        case '2':\n          this.$router.push('/jobs');\n          break;\n        case '3':\n          this.$router.push('/companies');\n          break;\n        case '4':\n          this.$router.push('/login');\n          break;\n        case '5':\n          this.$router.push('/register');\n          break;\n        case '6-1':\n          this.$router.push('/user');\n          break;\n        case '6-2':\n          this.$router.push('/admin');\n          break;\n      }\n    },\n    logout() {\n      this.$store.dispatch('logout');\n      this.$message.success('退出登录成功');\n      this.$router.push('/');\n    },\n    goToJobDetail(id) {\n      this.$router.push(`/jobs/${id}`);\n    },\n    goToCompanyDetail(id) {\n      this.$router.push(`/companies/${id}`);\n    },\n    getHotJobs() {\n      this.$axios.get('/jobs')\n        .then(response => {\n          // 模拟企业名称，实际项目中需要关联企业表\n          this.hotJobs = response.data.map(job => ({\n            ...job,\n            company: '企业名称 ' + Math.floor(Math.random() * 100)\n          }));\n        })\n        .catch(error => {\n          console.error('获取岗位失败:', error);\n        });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.home {\n  padding-bottom: 50px;\n}\n.hot-jobs,\n.hot-companies {\n  margin: 30px;\n}\n.job-card,\n.company-card {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n}\n.job-info,\n.company-info {\n  margin-bottom: 15px;\n}\n.company-logo {\n  width: 100px;\n  height: 100px;\n  border-radius: 50%;\n  margin: 0 auto 15px;\n}\n\n.job-list {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  margin-top: 20px;\n}\n\n.job-card {\n  width: 30%;\n  margin-bottom: 20px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.job-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n}\n\n.job-meta {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 10px;\n}\n\n.salary {\n  color: #ff7e00;\n  font-weight: bold;\n}\n</style>"],"mappings":"AAkFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,OAAA,GACA;QAAAC,EAAA;QAAAC,KAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,QAAA;MAAA,GACA;QAAAJ,EAAA;QAAAC,KAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,QAAA;MAAA,GACA;QAAAJ,EAAA;QAAAC,KAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,QAAA;MAAA,GACA;QAAAJ,EAAA;QAAAC,KAAA;QAAAC,OAAA;QAAAC,MAAA;QAAAC,QAAA;MAAA,EACA;MACAC,YAAA,GACA;QAAAL,EAAA;QAAAJ,IAAA;QAAAU,QAAA;QAAAC,IAAA;MAAA,GACA;QAAAP,EAAA;QAAAJ,IAAA;QAAAU,QAAA;QAAAC,IAAA;MAAA,GACA;QAAAP,EAAA;QAAAJ,IAAA;QAAAU,QAAA;QAAAC,IAAA;MAAA,GACA;QAAAP,EAAA;QAAAJ,IAAA;QAAAU,QAAA;QAAAC,IAAA;MAAA;IAEA;EACA;EACAC,QAAA;IACAC,WAAA;MACA,YAAAC,MAAA,CAAAC,OAA